<div class = "container">
  <div class = "row">
    <div class = "mx-auto text-center col-md-6 col-md-offset-3">
      <h1>スケジュール</h1>
      <table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>場所</th>
            <th>日時</th>
          </tr>
        </thead>
        <tbody>
          <% @events.each do |event| %>
            <tr>
              <td><%= event.place %></td>
              <td><%= event.event_date %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>